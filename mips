  .text
main:
  
   jal Convert #jump and link
   
   li $v0, 8
   la $a0, input # This block gets the users input
   li $a1, 1000
   syscall
   
   li $v0, 4
   la $a0, input # This block prints the users input
   syscall
   
   move $s0, $a0
   
loop:
  
  lb $a0, 0($s0)
  addi $s0, $s0, 1
  beq $a0, 32, loop
